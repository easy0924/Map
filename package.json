        "react-dom": "^17.0.2",
        "react-leaflet": "^3.2.0",
        "react-relay": "^11.0.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "web-vitals": "^1.0.1"
    },
    "devDependencies": {
        "@types/leaflet": "^1.7.1",
